{
  "name": "tamlynscore",
  "version": "0.1.0",
  "description": "This project is a rewrite of [archery-scoring](https://www.github.com/mjtamlyn/archery-scoring). It can be considered a version 2 of that project, rewritten to be more flexible, more reusable, and more powerful. It will also have a much more performant and interactive front end with a backing API, allowing development of companion applications.",
  "main": "index.js",
  "scripts": {
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mjtamlyn/tamlynscore.git"
  },
  "author": "Marc Tamlyn",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/mjtamlyn/tamlynscore/issues"
  },
  "homepage": "https://github.com/mjtamlyn/tamlynscore#readme",
  "devDependencies": {
    "jest-cli": "^13.0.0"
  },
  "dependencies": {
    "babel-core": "^6.10.4",
    "babel-jest": "^13.0.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-relay-plugin": "^0.9.1",
    "babel-relay-plugin-loader": "^0.9.0",
    "react": "^15.1.0",
    "react-addons-test-utils": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-relay": "^0.9.1",
    "webpack": "^1.13.1",
    "webpack-bundle-tracker": "0.0.93"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ],
    "plugins": [
      "babel-relay-plugin-loader"
    ]
  },
  "jest": {
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom/",
      "<rootDir>/node_modules/react-addons-test-utils/"
    ]
  },
  "metadata": {
    "graphql": {
      "schema": "./static/data/schema.json"
    }
  }
}
